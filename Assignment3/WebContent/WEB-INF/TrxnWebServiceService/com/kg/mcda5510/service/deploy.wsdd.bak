<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TrxnWebServiceService WSDL service -->

  <service name="TrxnWebService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.mcda5510.kg.com"/>
      <parameter name="wsdlServiceElement" value="TrxnWebServiceService"/>
      <parameter name="schemaQualified" value="http://service.mcda5510.kg.com"/>
      <parameter name="wsdlServicePort" value="TrxnWebService"/>
      <parameter name="className" value="com.kg.mcda5510.service.TrxnWebServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TrxnWebService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getTransaction" qname="operNS:getTransaction" xmlns:operNS="http://service.mcda5510.kg.com" soapAction="" >
        <parameter qname="pns:trxnID" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="createTransaction" qname="operNS:createTransaction" xmlns:operNS="http://service.mcda5510.kg.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Name" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CardType" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CardNumber" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:unitPrice" xmlns:pns="http://service.mcda5510.kg.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:qty" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:expDate" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="updateTransaction" qname="operNS:updateTransaction" xmlns:operNS="http://service.mcda5510.kg.com" soapAction="" >
        <parameter qname="pns:oldid" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:id" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Name" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CardType" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CardNumber" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:unitPrice" xmlns:pns="http://service.mcda5510.kg.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:qty" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:expDate" xmlns:pns="http://service.mcda5510.kg.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="removeTransaction" qname="operNS:removeTransaction" xmlns:operNS="http://service.mcda5510.kg.com" soapAction="" >
        <parameter qname="pns:trxnID" xmlns:pns="http://service.mcda5510.kg.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="updateTransaction removeTransaction createTransaction getTransaction"/>

  </service>
</deployment>
